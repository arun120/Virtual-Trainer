    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Exercises</servlet-name>
        <url-pattern>/Exercises</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
